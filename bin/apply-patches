#!/bin/sh
#set -x
#
# Apply patches
#
if [ ! -x bin/$(basename $0) ]
then
	echo "ERROR: Run this from top level"
	exit 1
fi
. ./config.sh


AGGR_MODULE=$(basename $ECLIPSE_AGGR)
BASELINE=".baseline.${AGGR_MODULE}"
if [ ! -d ${BASELINE} ]
then
	echo "ERROR: bin/prepare-tree needs to be run first"
	exit 1
fi

cat patches/patch-* | patch --directory ${AGGR_MODULE} --fuzz 0 -V none --quiet
